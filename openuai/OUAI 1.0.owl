                     swpo:Project ,
                     swpo:Publication ,
                     swpo:PublicationContainer ,
                     vivo:Course ,
                     vaem:Discipline ,
                     foaf:Organization ;
         rdfs:range rdfs:Literal ;
         rdfs:label "acronym"@en ,
