                   swpo:Project ,
                   swpo:Publication ,
                   swpo:PublicationContainer ,
                   vivo:Course ,
                   vaem:Discipline ,
                   foaf:Organization ;
       rdfs:range xsd:string ;
       rdfs:label "about"@en ,
                  "sobre"@pt .


