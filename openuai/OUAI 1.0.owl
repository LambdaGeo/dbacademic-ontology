#################################################################
#    Annotation properties
#################################################################

###  http://dbpedia.org/ontology/birthDate
dbo:birthDate rdf:type owl:AnnotationProperty ;
              rdfs:range xsd:dateTime ;
              rdfs:domain dbo:Professor ,
                          swpo:Researcher ,
                          swpo:Student ,
                          foaf:Person .


###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/date
dcterms:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/identifier
dcterms:identifier rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/subject
dcterms:subject rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty ;
              rdfs:range xsd:string ;
              rdfs:domain dbo:Award ,
                          dbo:ResearchProject ,
                          dbo:Software ,
                          <http://openuai.ifpb.edu.br/ontology/1.0/ouai#GeneralArea> ,
                          ouai:Area ,
                          ouai:ExtensionProject ,
                          ouai:GeneralArea ,
                          ouai:InnovationProject ,
                          ouai:Subarea ,
                          ouai:Subject ,
                          <http://purl.org/ontology/bibo/Patent> ,
                          swpo:Article ,
                          swpo:Event ,
                          swpo:Group ,
                          swpo:Inproceedings ,
                          swpo:Journal ,
                          swpo:Proceedings ,
                          swpo:Project ,
                          swpo:Publication ,
                          swpo:PublicationContainer ,
                          swpo:Techreport ,
                          vivo:Course ,
                          vaem:Discipline ,
                          foaf:Organization .


#################################################################
#    Object Properties
#################################################################

###  http://dbpedia.org/ontology/editor
dbo:editor rdf:type owl:ObjectProperty ;
           rdfs:domain swpo:PublicationContainer ;
           rdfs:range foaf:Person .


###  http://dbpedia.org/ontology/projectCoordinator
dbo:projectCoordinator rdf:type owl:ObjectProperty ;
                       rdfs:domain swpo:Project ;
                       rdfs:range dbo:Professor ,
                                  swpo:Researcher .


###  http://purl.org/dc/terms/hasLanguage
dcterms:hasLanguage rdf:type owl:ObjectProperty ;
                    rdfs:domain swpo:Publication ,
                                foaf:Person ;
                    rdfs:range dcterms:LinguisticSystem ;
                    rdfs:label "has language"@en ,
                               "tem idioma"@pt .


###  http://purl.org/ontology/openuai#containsInProceedings
:containsInProceedings rdf:type owl:ObjectProperty ;
                       owl:inverseOf swpo:containedInProceedings ;
                       rdfs:domain swpo:Proceedings ;
                       rdfs:range swpo:Inproceedings ;
                       rdfs:label "contains in proceedings"@en ,
                                  "contém artigos em anais"@pt .


###  http://purl.org/ontology/openuai#hasAdvisor
:hasAdvisor rdf:type owl:ObjectProperty ;
            owl:inverseOf :isAdvisorIn ;
            rdfs:domain swpo:Project ;
            rdfs:range dbo:Professor ,
                       swpo:Researcher ;
            rdfs:label "has advisor"@en ,
                       "tem orientador"@pt .


###  http://purl.org/ontology/openuai#hasBibliography
:hasBibliography rdf:type owl:ObjectProperty ;
                 rdfs:domain vaem:Discipline ;
                 rdfs:range :Bibliography .


###  http://purl.org/ontology/openuai#hasContent
:hasContent rdf:type owl:ObjectProperty ;
            rdfs:domain vaem:Discipline ;
            rdfs:range :Content .


###  http://purl.org/ontology/openuai#hasCoordinator
:hasCoordinator rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                owl:inverseOf :isCoordinatorIn ;
                rdfs:domain dbo:ResearchProject ,
                            :ExtensionProject ,
                            :InnovationProject ,
                            swpo:Project ;
                rdfs:range dbo:Professor ,
                           swpo:Researcher ;
                rdfs:label "has coordinator"@en ,
                           "tem coordenador"@pt .


###  http://purl.org/ontology/openuai#hasDegree
:hasDegree rdf:type owl:ObjectProperty ;
           rdfs:domain foaf:Person ;
           rdfs:range :AcademicDegree ;
           rdfs:label "has degree"@en ,
                      "tem grau academico"@pt .


###  http://purl.org/ontology/openuai#hasExpertiseIn
:hasExpertiseIn rdf:type owl:ObjectProperty ;
                rdfs:domain foaf:Person ;
                rdfs:range :Subject ;
                rdfs:label "has expertise in"@en ,
                           "tem expertise em"@pt .


###  http://purl.org/ontology/openuai#hasInterestIn
:hasInterestIn rdf:type owl:ObjectProperty ;
               rdfs:domain foaf:Person ;
               rdfs:range :GeneralArea ;
               rdfs:label "has insterest in"@en ,
                          "tem interesse em"@pt .


###  http://purl.org/ontology/openuai#hasKnowledgeArea
:hasKnowledgeArea rdf:type owl:ObjectProperty ;
                  rdfs:domain <http://dbpedia.org/resource/EducationalInstitution> ,
                              swpo:Group ,
                              swpo:Project ,
                              swpo:Publication ,
                              swpo:PublicationContainer ,
                              foaf:Person ;
                  rdfs:range :GeneralArea ;
                  rdfs:label "has knowledge area"@en ,
                             "tem área de conhecimento"@pt .


###  http://purl.org/ontology/openuai#hasParticipatedIn
:hasParticipatedIn rdf:type owl:ObjectProperty ;
                   rdfs:domain foaf:Person ;
                   rdfs:range :ExaminingBoard ,
                              swpo:Event ,
                              swpo:Group ,
                              swpo:Project ;
                   rdfs:label "has participated in"@en ,
                              "participou em"@pt .


###  http://purl.org/ontology/openuai#hasProject
:hasProject rdf:type owl:ObjectProperty ;
            owl:inverseOf foaf:isPartOf ;
            rdfs:domain swpo:Group ;
            rdfs:range swpo:Project ;
            rdfs:label "has project"@en ,
                       "tem pojeto"@pt .


###  http://purl.org/ontology/openuai#hasPublication
:hasPublication rdf:type owl:ObjectProperty ;
                owl:inverseOf swpo:hasAuthors ;
                rdfs:domain foaf:Person ;
                rdfs:range swpo:Publication ;
                rdfs:label "has publication"@en ,
                           "tem publicação"@pt .


###  http://purl.org/ontology/openuai#hasScholarshipOf
:hasScholarshipOf rdf:type owl:ObjectProperty ;
                  rdfs:domain swpo:Student ;
                  rdfs:range <http://dbpedia.org/resource/EducationalInstitution> ,
                             swpo:ResearchInstitute ;
                  rdfs:label "has scholarship of"@en ,
                             "tem bolsa de"@pt .


###  http://purl.org/ontology/openuai#hasSoftwareRegistration
:hasSoftwareRegistration rdf:type owl:ObjectProperty ;
                         rdfs:domain foaf:Person ;
                         rdfs:range :SoftwareRegistration .


###  http://purl.org/ontology/openuai#hasStudent
:hasStudent rdf:type owl:ObjectProperty ;
            owl:inverseOf :isVolunteerIn ;
            rdfs:domain swpo:Project ;
            rdfs:range swpo:Student ;
            rdfs:label "has student"@en ,
                       "tem estudante"@pt .


###  http://purl.org/ontology/openuai#hasSubjectCourse
:hasSubjectCourse rdf:type owl:ObjectProperty ;
                  rdfs:domain vivo:Course ;
                  rdfs:range vaem:Discipline ;
                  rdfs:label "has Subject Course"@en ,
                             "tem disciplinas"@pt .


###  http://purl.org/ontology/openuai#isAdvisorIn
:isAdvisorIn rdf:type owl:ObjectProperty ;
             rdfs:domain dbo:Professor ,
                         swpo:Researcher ;
             rdfs:range swpo:Publication ;
             rdfs:label "is advisor em"@en ,
                        "é orientador em"@pt .


###  http://purl.org/ontology/openuai#isAuthorOf
:isAuthorOf rdf:type owl:ObjectProperty ;
            owl:inverseOf swpo:hasAuthors ;
            rdfs:domain swpo:Group ,
                        foaf:Organization ,
                        foaf:Person ;
            rdfs:range dbo:Software ,
                       <http://purl.org/ontology/bibo/Patent> ,
                       swpo:Project ,
                       swpo:Publication ;
            rdfs:label "is author of"@en ,
                       "é autor de"@pt .


###  http://purl.org/ontology/openuai#isCoordinatorIn
:isCoordinatorIn rdf:type owl:ObjectProperty ;
                 rdfs:domain dbo:Professor ,
                             swpo:Researcher ;
                 rdfs:range swpo:Event ,
                            swpo:Group ,
                            swpo:Project ,
                            vivo:Course ;
                 rdfs:label "is coordinator in"@en ,
                            "é coordenador em"@pt .


###  http://purl.org/ontology/openuai#isMemberOf
:isMemberOf rdf:type owl:ObjectProperty ;
            rdfs:domain foaf:Person ;
            rdfs:range <http://dbpedia.org/resource/EducationalInstitution> ,
                       foaf:Organization .


###  http://purl.org/ontology/openuai#isScholarshipHolderIn
:isScholarshipHolderIn rdf:type owl:ObjectProperty ;
                       rdfs:domain foaf:Person ;
                       rdfs:range swpo:Project .


###  http://purl.org/ontology/openuai#isStudying
:isStudying rdf:type owl:ObjectProperty ;
            rdfs:domain swpo:Student ;
            rdfs:range vivo:Course ;
            rdfs:label "está estudando"@pt ,
                       "is studying"@en .


###  http://purl.org/ontology/openuai#isVolunteerIn
:isVolunteerIn rdf:type owl:ObjectProperty ;
               rdfs:domain swpo:Student ;
               rdfs:range swpo:Project ;
               rdfs:label "is volunteer in"@en ,
                          "é voluntário em"@pt .


###  http://purl.org/ontology/openuai#reviewedBy
:reviewedBy rdf:type owl:ObjectProperty ;
            rdfs:domain swpo:Publication ;
            rdfs:range foaf:Person .


###  http://purl.org/ontology/openuai#studiesAt
:studiesAt rdf:type owl:ObjectProperty ;
           rdfs:domain swpo:Student ;
           rdfs:range <http://dbpedia.org/resource/EducationalInstitution> ;
           rdfs:label "estuda em"@pt ,
                      "studies at"@en .


###  http://purl.org/ontology/openuai#teachesAt
:teachesAt rdf:type owl:ObjectProperty ;
           rdfs:domain dbo:Professor ;
           rdfs:range <http://dbpedia.org/resource/EducationalInstitution> ;
           rdfs:label "ensina em"@pt ,
                      "teaches at"@en .


###  http://purl.org/ontology/openuai#teachesDiscipline
:teachesDiscipline rdf:type owl:ObjectProperty ;
                   rdfs:domain dbo:Professor ;
                   rdfs:range vaem:Discipline ;
                   rdfs:label "ensina disciplina"@pt ,
                              "teaches discipline"@en .


###  http://sw-portal.deri.org/ontologies/swportal#containedInProceedings
swpo:containedInProceedings rdf:type owl:ObjectProperty ;
                            rdfs:domain swpo:Inproceedings ;
                            rdfs:range swpo:Proceedings ;
                            rdfs:label "contained in proceedings"@en ,
                                       "em anais"@pt .


###  http://sw-portal.deri.org/ontologies/swportal#containsArticles
swpo:containsArticles rdf:type owl:ObjectProperty ;
                      owl:inverseOf swpo:publishedBy ;
                      rdfs:domain swpo:PublicationContainer ;
                      rdfs:range swpo:Publication ;
                      rdfs:label "contains articles"@en ,
                                 "contém artigos"@pt .


###  http://sw-portal.deri.org/ontologies/swportal#hasAuthors
swpo:hasAuthors rdf:type owl:ObjectProperty ;
                rdfs:domain swpo:Publication ;
                rdfs:range foaf:Person ;
                rdfs:label "has authors"@en ,
                           "tem autores"@pt .


###  http://sw-portal.deri.org/ontologies/swportal#organizedby
swpo:organizedby rdf:type owl:ObjectProperty ;
                 owl:inverseOf swpo:organizes ;
                 rdfs:domain swpo:Event ;
                 rdfs:range swpo:Group ,
                            foaf:Organization ,
                            foaf:Person .


###  http://sw-portal.deri.org/ontologies/swportal#organizes
swpo:organizes rdf:type owl:ObjectProperty ;
               rdfs:domain swpo:Group ,
                           foaf:Organization ,
                           foaf:Person ;
               rdfs:range swpo:Event .


###  http://sw-portal.deri.org/ontologies/swportal#publishedBy
swpo:publishedBy rdf:type owl:ObjectProperty ;
                 rdfs:domain swpo:Publication ;
                 rdfs:range swpo:Group ,
                            foaf:Organization ,
                            foaf:Person ;
                 rdfs:label "publicado por"@pt ,
                            "published by"@en .


###  http://xmlns.com/foaf/0.1/isPartOf
foaf:isPartOf rdf:type owl:ObjectProperty ;
              rdfs:domain swpo:Project ;
              rdfs:range swpo:Group ;
              rdfs:label "faz parte de"@pt ,
                         "is parte of"@en .


