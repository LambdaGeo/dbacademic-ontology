#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Award
dbo:Award rdf:type owl:Class ;
          rdfs:comment "Award won by a Person, Musical or other Work, RaceHorse, Building, etc"@en ,
                       "Prêmio ganhado por uma Pessoa, Musical ou outro Trabalho, RaceHorse, Edifício, etc"@pt ;
          rdfs:label "Award"@en ,
                     "Prêmio"@pt .


###  http://dbpedia.org/ontology/Professor
dbo:Professor rdf:type owl:Class ;
              rdfs:subClassOf foaf:Person ;
              rdfs:comment "A person whose role is Professor in a  Project. The Professor can be also a Researcher."@en ,
                           "Uma pessoa cujo papel é Professor num Projeto. O Professor pode ser também um Pesquisador."@pt ;
              rdfs:label "Professor"@en ,
                         "Professor"@pt .


###  http://dbpedia.org/ontology/ResearchProject
dbo:ResearchProject rdf:type owl:Class ;
                    rdfs:subClassOf swpo:Project ;
                    rdfs:comment "It aims to research some issues in a determined Knowledge Area, and topics."@en ,
                                 "Tem por objetivo pesquisar problemas de determinada Área de Conhecimento e tópicos."@pt ;
                    rdfs:label "Projeto de Pesquisa"@pt ,
                               "Research Project"@en .


###  http://dbpedia.org/ontology/Software
dbo:Software rdf:type owl:Class ;
             rdfs:comment "A set of logical components of a computer or data processing system; Program, routine or set of instructions that control the operation of a computer; Software."@en ,
                          "Conjunto de componentes lógicos de um computador ou sistema de processamento de dados; programa, rotina ou conjunto de instruções que controlam o funcionamento de um computador; suporte lógico."@pt ;
             rdfs:label "Software"@en ,
                        "Software"@pt .


###  http://dbpedia.org/resource/EducationalInstitution
<http://dbpedia.org/resource/EducationalInstitution> rdf:type owl:Class ;
                                                     rdfs:subClassOf foaf:Organization ;
                                                     rdfs:comment "An academic institution where a person belongs (or is part of), such as university, college, school."@en ,
                                                                  "Uma instituição acadêmica à qual uma pessoa pertente (ou faz parte), como universidade, faculdade, escola."@pt ;
                                                     rdfs:label "Educational Institution"@en ,
                                                                "Instituição Educacional"@pt .


###  http://linguistics-ontology.org/gold/Process
