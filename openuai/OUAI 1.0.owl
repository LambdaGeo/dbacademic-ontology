                          :AcademicDegree ,
                          :GeneralArea ,
                          swpo:Event ,
                          swpo:Group ,
                          swpo:Project ,
                          swpo:PublicationContainer ,
                          vivo:Course ,
                          vaem:Discipline ,
                          foaf:Organization ;
              rdfs:range rdfs:Literal ;
              owl:propertyDisjointWith :acronym ;
              rdfs:label "abbreviation"@en ,
                         "abreviação"@pt .


###  http://purl.org/ontology/openuai#about
:about rdf:type owl:DatatypeProperty ;
       rdfs:domain dbo:Award ,
                   <http://purl.org/ontology/bibo/Patent> ,
                   swpo:Event ,
