                <owl:allValuesFrom rdf:resource="http://www.aktors.org/ontology/portal#Organization-Size"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.aktors.org/ontology/portal#has-sub-unit"/>
