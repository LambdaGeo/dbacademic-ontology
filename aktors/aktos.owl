        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.aktors.org/ontology/portal#has-author"/>
                <owl:allValuesFrom rdf:resource="http://www.aktors.org/ontology/portal#Generic-Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.aktors.org/ontology/portal#has-date"/>
                <owl:allValuesFrom rdf:resource="http://www.aktors.org/ontology/support#Calendar-Date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
