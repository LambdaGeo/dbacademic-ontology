    <!-- http://www.aktors.org/ontology/portal#Serial-Publication -->

    <owl:Class rdf:about="http://www.aktors.org/ontology/portal#Serial-Publication">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.aktors.org/ontology/portal#Journal"/>
                    <rdf:Description rdf:about="http://www.aktors.org/ontology/portal#Magazine"/>
                    <rdf:Description rdf:about="http://www.aktors.org/ontology/portal#Newspaper"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.aktors.org/ontology/portal#Publication"/>
        <rdfs:comment>This used to be called periodical publication.  However, many periodicals do not appear at fixed intervals, which is why librarians refer to them as serials. So, we now use the concept of serial publication and the has-periodicity slot has been removed</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.aktors.org/ontology/portal</rdfs:isDefinedBy>
    </owl:Class>
    


