            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.aktors.org/ontology/portal#has-size"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:oneOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.aktors.org/ontology/portal#medium-size"/>
                            <rdf:Description rdf:about="http://www.aktors.org/ontology/portal#small-size"/>
