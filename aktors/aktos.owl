            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.aktors.org/ontology/portal#headed-by"/>
                <owl:allValuesFrom rdf:resource="http://www.aktors.org/ontology/portal#Affiliated-Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.aktors.org/ontology/portal#organization-part-of"/>
                <owl:allValuesFrom rdf:resource="http://www.aktors.org/ontology/portal#Organization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.aktors.org/ontology/portal#has-size"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An organization is a type of legal agent</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.aktors.org/ontology/portal</rdfs:isDefinedBy>
